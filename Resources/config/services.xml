        xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

        <services>

            <service id="sulu_developer_bundle.command.generate_document_tree" class="Sulu\Bundle\DeveloperBundle\Command\GenerateDocumentTreeCommand">
