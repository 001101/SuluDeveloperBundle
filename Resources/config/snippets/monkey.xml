<?xml version="1.0" ?>

<template xmlns="http://schemas.sulu.io/template/template"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://schemas.sulu.io/template/template http://schemas.sulu.io/template/template-1.0.xsd">

    <key>monkey</key>

    <meta>
        <title lang="de">Affe</title>
        <title lang="en">Monkey</title>
    </meta>

    <properties>
        <property name="title" type="text_line" mandatory="true">
            <meta>
                <title lang="de">Nam</title>
                <title lang="en">Name</title>
            </meta>

            <tag name="sulu.node.name"/>
        </property>

        <property name="description" type="text_editor">
            <meta>
                <title lang="de">Beschreibung</title>
                <title lang="en">Description</title>
            </meta>
        </property>

        <property name="born" type="date">
            <meta>
                <title lang="de">Born</title>
                <title lang="en">Born</title>
            </meta>
        </property>

        <property name="died" type="date">
            <meta>
                <title lang="de">Died</title>
                <title lang="en">Died</title>
            </meta>
        </property>

    </properties>
</template>
